<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/blurry"
    tools:context="me.dm7.barcodescanner.zbar.sample.Fin">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Fin"
        android:id="@+id/textView16"
        android:layout_above="@+id/textView17"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="66dp"
        android:textSize="36sp"
        android:textColor="#000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="User has been registered"
        android:id="@+id/textView17"
        android:layout_above="@+id/button26"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="95dp"
        android:textSize="36sp"
        android:textColor="#000" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Register Another"
        android:id="@+id/button26"
        android:onClick="Fin_Return"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
